<hibernate-mapping>
  <class lazy="false" name="de.fhb.jproject.data.Comment" table="`Comment`">
    <id column="`ID`" name="id" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <many-to-one access="field" cascade="lock" class="de.fhb.jproject.data.User" column="`User`" lazy="no-proxy" name="user" not-null="true">
		</many-to-one>
    <property column="`Entry`" lazy="false" name="entry" not-null="true" type="text"/>
    <one-to-one access="field" cascade="save-update,lock" class="de.fhb.jproject.data.CommentProject" lazy="no-proxy" name="commentProject"/>
    <one-to-one access="field" cascade="save-update,lock" class="de.fhb.jproject.data.CommentSourcecode" lazy="no-proxy" name="commentSourcecode"/>
    <one-to-one access="field" cascade="save-update,lock" class="de.fhb.jproject.data.CommentDocument" lazy="no-proxy" name="commentDocument"/>
    <one-to-one access="field" cascade="save-update,lock" class="de.fhb.jproject.data.CommentTask" lazy="no-proxy" name="commentTask"/>
  </class>
</hibernate-mapping>
