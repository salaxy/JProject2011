<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.fhb.jproject.data.CommentDocument" table="`CommentDocument`" lazy="false">
		<id name="documentId" column="`DocumentID`" type="integer">
			<generator class="foreign">
				<param name="property">document</param>
			</generator>
		</id>
		<one-to-one name="document" class="de.fhb.jproject.data.Document" cascade="all" constrained="true"/>
		<many-to-one name="comment" column="`CommentID`" class="de.fhb.jproject.data.Comment" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
