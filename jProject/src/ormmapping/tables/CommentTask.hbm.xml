<hibernate-mapping>
  <class lazy="false" name="de.fhb.jproject.data.CommentTask" table="`CommentTask`">
    <id column="`TaskID`" name="taskId" type="integer">
      <generator class="foreign">
        <param name="property">task</param>
      </generator>
    </id>
    <one-to-one cascade="all" class="de.fhb.jproject.data.Task" constrained="true" name="task"/>
    <many-to-one access="field" cascade="save-update,lock" class="de.fhb.jproject.data.Comment" column="`CommentID`" lazy="no-proxy" name="comment" not-null="true" unique="true">
		</many-to-one>
  </class>
</hibernate-mapping>
