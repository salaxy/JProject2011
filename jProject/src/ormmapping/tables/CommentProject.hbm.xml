<hibernate-mapping>
  <class lazy="false" name="de.fhb.jproject.data.CommentProject" table="`CommentProject`">
    <id column="`ProjectName`" name="projectNameId" type="string">
      <generator class="foreign">
        <param name="property">projectName</param>
      </generator>
    </id>
    <one-to-one cascade="all" class="de.fhb.jproject.data.Project" constrained="true" name="projectName"/>
    <many-to-one access="field" cascade="save-update,lock" class="de.fhb.jproject.data.Comment" column="`CommentID`" lazy="no-proxy" name="comment" not-null="true" unique="true">
		</many-to-one>
  </class>
</hibernate-mapping>
