<hibernate-mapping>
  <class lazy="false" name="de.fhb.jproject.data.Task" table="`Task`">
    <id column="`ID`" name="id" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <many-to-one access="field" cascade="lock" class="de.fhb.jproject.data.Project" column="`Project`" lazy="no-proxy" name="project" not-null="true">
		</many-to-one>
    <property column="`Titel`" lazy="false" length="255" name="titel" not-null="true" type="string"/>
    <property column="`Aufgabenstellung`" lazy="false" name="aufgabenstellung" not-null="true" type="text"/>
    <property lazy="false" length="1" name="done" not-null="true" type="boolean">
      <column default="0" name="`Done`"/>
    </property>
    <many-to-one access="field" cascade="lock" class="de.fhb.jproject.data.Termin" column="`TerminID`" lazy="no-proxy" name="termin" not-null="false">
		</many-to-one>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_MemberUser" table="`TODO`">
      <key column="`TaskID`" not-null="true"/>
      <many-to-many class="de.fhb.jproject.data.Member">
        <column name="`MemberUser`"/>
        <column name="`MemberProject`"/>
      </many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_CommentTask">
      <key column="`TaskID`" not-null="true"/>
      <one-to-many class="de.fhb.jproject.data.CommentTask"/>
    </set>
  </class>
</hibernate-mapping>
