<hibernate-mapping>
  <class lazy="false" name="de.fhb.jproject.data.Termin" table="`Termin`">
    <id column="`ID`" name="id" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`Termin`" lazy="false" name="termin" not-null="true" type="date"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Task">
      <key column="`TerminID`" not-null="false"/>
      <one-to-many class="de.fhb.jproject.data.Task"/>
    </set>
  </class>
</hibernate-mapping>
