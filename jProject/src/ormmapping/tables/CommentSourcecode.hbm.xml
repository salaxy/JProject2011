<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
Licensee: DuKe TeAm
License Type: Purchased
-->
<hibernate-mapping>
  <class lazy="false" name="de.fhb.jproject.data.CommentSourcecode" table="`CommentSourcecode`">
    <id column="`CommentID`" name="commentId" type="integer">
      <generator class="foreign">
        <param name="property">comment</param>
      </generator>
    </id>
    <one-to-one cascade="all" class="de.fhb.jproject.data.Comment" constrained="true" name="comment"/>
    <many-to-one access="field" cascade="lock" class="de.fhb.jproject.data.Sourcecode" column="`SourcecodeID`" lazy="no-proxy" name="sourcecode" not-null="true">
		</many-to-one>
  </class>
</hibernate-mapping>
