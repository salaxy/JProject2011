<hibernate-mapping>
  <class lazy="false" name="de.fhb.jproject.data.Document" table="`Document`">
    <id column="`ID`" name="id" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <many-to-one access="field" cascade="lock" class="de.fhb.jproject.data.Project" column="`ProjectName`" lazy="no-proxy" name="projectName" not-null="true">
		</many-to-one>
    <property column="`Dateiname`" lazy="false" length="255" name="dateiname" not-null="true" type="string"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Comment_Document">
      <key column="`DocumentID`" not-null="true"/>
      <one-to-many class="de.fhb.jproject.data.CommentDocument"/>
    </set>
  </class>
</hibernate-mapping>
